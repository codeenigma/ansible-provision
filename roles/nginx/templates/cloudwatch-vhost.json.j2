{
  "logs": {
    "logs_collected": {
      "files": {
        "collect_list": [
          {
            "file_path": "{{ domain.access_log }}",
            "log_group_name": "nginx-access",
{% if nginx.log_group_prefix is defined and nginx.log_group_prefix|length %}
            "log_group_name": "{{nginx.log_group_prefix}}nginx-access",
{% else %}
						"log_group_name": "nginx-access",
{% endif %}
            "log_stream_name": "{{ domain.log_stream_name }}"
          },
          {
            "file_path": "{{ domain.error_log }}",
{% if nginx.log_group_prefix is defined and nginx.log_group_prefix|length %}
            "log_group_name": "{{nginx.log_group_prefix}}nginx-error",
{% else %}
						"log_group_name": "nginx-error",
{% endif %}
            "log_stream_name": "{{ domain.log_stream_name }}"
          }
        ]
      }
    }
  }
}